        "twig/string-extra": "^3.22.1",
        "twig/twig": "^3.22.1"
    },
    "require-dev": {
        "doctrine/coding-standard": "^14.0",
        "phpstan/phpstan": "^2.1.33",
        "phpstan/phpstan-deprecation-rules": "^2.0.3",
        "phpstan/phpstan-doctrine": "^2.0.12",
        "phpstan/phpstan-phpunit": "^2.0.10",
        "phpstan/phpstan-strict-rules": "^2.0.7",
        "phpstan/phpstan-symfony": "^2.0.9",
        "phpunit/phpunit": "^12.5.3",
        "rector/rector": "^2.2.14",
        "roave/security-advisories": "dev-master",
        "shipmonk/composer-dependency-analyser": "^1.8.4",
        "squizlabs/php_codesniffer": "^4.0.1",
        "symfony/stopwatch": "^8.0",
        "symfony/web-profiler-bundle": "^8.0",
        "symplify/config-transformer": "^12.4.3",
        "thecodingmachine/phpstan-safe-rule": "^1.4.3"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "prepare-test": [
            "php bin/console cache:clear --env=test --no-warmup --ansi",
            "php bin/console cache:warmup --env=test --ansi --no-optional-warmers"
        ],
        "check": [
            "composer check-platform-reqs -n --ansi",
            "composer validate --strict -n --ansi",
            "@cda",
            "@cs-check-all",
            "@test"
        ],
        "check-pre-push": [
            "composer check-platform-reqs -n --ansi",
            "composer validate --strict -n --ansi",
            "@cs-check-diff",
            "@lint-twig",
            "@lint-yaml",
            "@lint-container",
            "@test"
        ],
        "test": [
            "@prepare-test",
            "@phpstan",
            "@phpunit"
        ],
        "test-phpunit": [
            "@prepare-test",
            "@phpunit"
        ],
        "test-phpstan": [
            "@prepare-test",
            "@phpstan"
        ],
        "frankenphp-restart": "curl -X POST http://localhost:2019/frankenphp/workers/restart",
        "fos-js-dump": "php bin/console fos:js-routing:dump --format=json --target=public/js/fos_js_routes.json",
        "cda": "vendor/bin/composer-dependency-analyser --config=./etc/tools/composer-dependency-analyser.php",
        "phpstan": "phpstan analyse --ansi -c ./etc/tools/phpstan.neon",
        "phpstan-update-baseline": "@phpstan --generate-baseline ./etc/tools/phpstan-baseline.php",
        "phpunit": "phpunit --colors=always -c ./etc/tools/phpunit.xml",
        "cs-check": "@cs-check-all --filter=GitModified",
        "cs-check-all": "phpcs -s --standard=./etc/tools/phpcs.xml",
        "cs-fix": "@cs-fix-all --filter=GitModified",
        "cs-fix-all": "phpcbf --standard=./etc/tools/phpcs.xml",
        "lint-twig": "php bin/console lint:twig src templates --show-deprecations --ansi",
        "lint-yaml": "php bin/console lint:yaml config src translations --parse-tags --ansi",
        "lint-container": "php bin/console lint:container --ansi",
        "rector": "@rector-fix --dry-run",
        "rector-fix": "vendor/bin/rector process --config=./etc/tools/rector.php --ansi",
        "install-pnpm": "sudo npm install -g pnpm",
        "install-tunnel": "sudo npm install -g localtunnel",
        "install-codestandards": "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run",
        "tunnel": [
            "Composer\\Config::disableProcessTimeout",
            "lt --port 80 --subdomain svtr"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.4|8.0.*"
        }
    }
}
