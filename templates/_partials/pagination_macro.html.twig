{% if preElements is not null %}
    {{ preElements|raw }}
{% endif %}

<twig:bs:button:group size="sm" class="me-1">
    {% if enableLink is not null and disableLink is not null %}
        {% if object.enabled %}
            <twig:bs:button type="success" href="{{ disableLink }}" icon="fa6-solid:circle" />
        {% else %}
            <twig:bs:button type="danger" href="{{ enableLink }}" icon="fa6-solid:circle" />
        {% endif %}
    {% endif %}
    {% if editLink is not null %}
        <twig:bs:button type="primary" href="{{ editLink }}" icon="fa6-solid:pencil" />
    {% endif %}
</twig:bs:button:group>
{% if removeLink is not null %}
    <twig:bs:button:group size="sm" class="me-1">
        <twig:bs:button type="danger" href="{{ removeLink }}" icon="fa6-solid:trash" />
{#        {{ stimulus_controller('confirmation') }}#}
    </twig:bs:button:group>
{% endif %}

{% if postElements is not null %}
    {{ postElements|raw }}
{% endif %}
