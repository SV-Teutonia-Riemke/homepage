{% extends '_layout/layout.html.twig' %}

{% block pageSubBody %}
    {% for article in articles %}
        <div class="card mb-3">
            <div class="card-header">
                <div class="card-title">{{ article.title }}</div>
            </div>
            <div class="card-body">{{ article.content|raw }}</div>
        </div>
    {% endfor %}
{% endblock %}
