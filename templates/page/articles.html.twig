<turbo-frame id="the_frame_id">
    {% for article in articles %}
        <div class="card">
            <div class="card-header">
                <div class="card-title">{{ article.title }}</div>
            </div>
            <div class="card-body">{{ article.content|raw }}</div>
        </div>
    {% endfor %}
</turbo-frame>
