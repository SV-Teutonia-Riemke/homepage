{% extends '@admin/_layout/simple_form.html.twig' %}

{% block javascripts %}
    {{ parent() }}
    {{ vite_entry_script_tags('monaco') }}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {{ vite_entry_link_tags('monaco') }}
{% endblock %}

{% set pageTitle = 'Seite' %}
{% set formCancelLink = path('app_admin_page_index') %}

{% block pageBody %}
    <div class="card">
        <div class="card-header">
            <h3 class="card-title">Seite</h3>
        </div>
        <div class="card-body">
            {{ form(form) }}
        </div>
    </div>
{% endblock %}
